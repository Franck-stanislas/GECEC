<div class="col-md-12 justify-content-center">
    {{ form_start(form) }}
        <div class="row">
            <div class="col-md-4 mb-3">
                <div>
                    <label for="first_name">Code de la Commune</label>
                    {{ form_widget(form.code) }}
                </div>
            </div>
            <div class="col-md-4 mb-3">
                <div>
                    <label for="first_name">Nom de la Commune</label>
                    {{ form_widget(form.commune) }}
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-md-4 mb-3">
                <div>
                    <label for="first_name">Nom de l'officier</label>
                    {{ form_widget(form.nom) }}
                </div>
            </div>
            <div class="col-md-4 mb-3">
                <div>
                    <label for="first_name">Prenom de l'officier</label>
                    {{ form_widget(form.prenom) }}
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-4 mb-3">
                <div>
                    <label for="first_name">Email</label>
                    {{ form_widget(form.email) }}
                </div>
            </div>
            <div class="col-md-4 mb-3">
                <div>
                    <label for="first_name">Poste</label>
                    {{ form_widget(form.poste) }}
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12 mb-3">
                <div>
                    <label for="first_name">Poste</label>
                    {{ form_widget(form.imageFile) }}
                </div>
            </div>
        </div>

        <div class="btn-action">
            <input type="reset" value="Annuler" id="btn-cancel">
            <button class="btn" id="btn-save">{{ button_label|default('Enregistrer') }}</button>
        </div>

    {{ form_end(form) }}
</div>
